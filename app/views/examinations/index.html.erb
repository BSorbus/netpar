<p id="notice"><%= notice %></p>

<h1>Listing Examinations</h1>

<table>
  <thead>
    <tr>
      <th>Examination category</th>
      <th>Division</th>
      <th>Examination resoult</th>
      <th>Exam</th>
      <th>Customer</th>
      <th>Note</th>
      <th>Category</th>
      <th>Exam</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @examinations.each do |examination| %>
      <tr>
        <td><%= examination.examination_category %></td>
        <td><%= examination.division %></td>
        <td><%= examination.examination_resoult %></td>
        <td><%= examination.exam %></td>
        <td><%= examination.customer %></td>
        <td><%= examination.note %></td>
        <td><%= examination.category %></td>
        <td><%= examination.exam %></td>
        <td><%= examination.user %></td>
        <td><%= link_to 'Show', examination %></td>
        <td><%= link_to 'Edit', edit_examination_path(examination) %></td>
        <td><%= link_to 'Destroy', examination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Examination', new_examination_path %>
