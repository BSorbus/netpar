<% content_for :upper_part_page do %>

  <!-- Half Page Image Background Carousel Header -->
  <div id="myCarousel" class="carousel slide">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
      <li data-target="#myCarousel" data-slide-to="3"></li>
      <li data-target="#myCarousel" data-slide-to="4"></li>
    </ol>

    <!-- Wrapper for Slides -->
    <div class="carousel-inner">
        <div class="item active">
          <div class="fill" style="background-image:url(<%= asset_path '1.jpg' %>)"></div>
          <div class="carousel-caption">
            <h3>Nie ma rozwiązań wiecznie trwałych</h3>
            <p>Po 15 latach eksploatacji Systemu NetPAR 2000...</p>
          </div>
        </div>
        <div class="item">
          <!-- Set the second background image using inline CSS below.
          <div class="fill" style="background-image:url('http://placehold.it/1900x1080&text=Wszędzie');"></div> -->
          <div class="fill" style="background-image:url(<%= asset_path '2.jpg' %>)"></div>
          <div class="carousel-caption">
            <h3>Czas na ulepszone ewidencjonowanie dokumentów</h3>
            <p>Tworzenie załączników, zapisywanie skanów, plików pdf, itp.</p>
          </div>
        </div>
        <div class="item">
          <!-- Set the next background image using inline CSS below. -->
          <div class="fill" style="background-image:url(<%= asset_path '3.jpg' %>)"></div>
          <div class="carousel-caption">
            <h3>Czas na wykorzystanie nowoczesnych technologii</h3>
            <p>Dostęp do danych przez przeglądarkę bez konieczności instalowania programu i kłopotliwych aktualizacji</p>
          </div>
        </div>
        <div class="item">
          <!-- Set the next background image using inline CSS below. -->
          <div class="fill" style="background-image:url(<%= asset_path '4.jpg' %>)"></div>
          <div class="carousel-caption">
            <h3>Czas na nowe spojrzenie</h3>
            <p>Wspólny rejestr dla wszystkich Delegatur</p>
          </div>
        </div>
        <div class="item">
          <!-- Set the next background image using inline CSS below. -->
          <div class="fill" style="background-image:url(<%= asset_path '5.jpg' %>)"></div>
          <div class="carousel-caption">
            <h2>...czas na nowy NETPAR 2015!</h2>
          </div>
        </div>
      </div> <!-- /div class="carousel-inner" -->

      <!-- Controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="icon-prev"></span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="icon-next"></span>
      </a>

  </div>


  <!-- Script to Activate the Carousel -->
  <script>
  $('.carousel').carousel({
      interval: 5000 //changes the speed
  })
  </script>

<% end %><!--/ content_for :upper_part_page -->



