<div class="panel-group" id="myaccordion">

  <!-- Dane -->
  <div class="row">
    <div class="col-sm-12">

      <div class="panel panel-certificate">
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_data">
          <div class="panel-heading">
            <div class="panel-title">
              <span class='<%= t("pages.charts_certificates.model_icon") %>'></span>&nbsp;
              <span class='<%= t("pages.exam_#{params[:category_service]}.model_icon") %>'></span>&nbsp;
              <span><%=t "pages.exam_#{params[:category_service]}.statistic_filter.title"%></span>
              <span><i class="indicator glyphicon glyphicon-chevron-down  pull-right"></i></span>
            </div> 
          </div>
        </a>
        <div id="collapse_data" class="panel-collapse collapse in">
          <div class="panel-body">         

            <%= render 'data_charts', input_disabled: true %>

          </div> <!-- /panel-body -->
        </div> <!-- /id="collapse_data" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-12-->
  </div> <!-- /row -->

</div> <!-- /class="panel-group" id="myaccordion" -->
