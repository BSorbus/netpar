<p id="notice"><%= notice %></p>

<h1>Listing Esod Internal Letters</h1>

<table>
  <thead>
    <tr>
      <th>Nrid</th>
      <th>Numer ewidencyjny</th>
      <th>Tytul</th>
      <th>Uwagi</th>
      <th>Identyfikator rodzaju dokumentu wewnetrznego</th>
      <th>Identyfikator typu dcmd</th>
      <th>Identyfikator dostepnosci dokumentu</th>
      <th>Pelna wersja cyfrowa</th>
      <th>Id zalozyl</th>
      <th>Id aktualizowal</th>
      <th>Data zalozenia</th>
      <th>Data aktualizacji</th>
      <th>Initialized from esod</th>
      <th>Netpar user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @esod_internal_letters.each do |esod_internal_letter| %>
      <tr>
        <td><%= esod_internal_letter.nrid %></td>
        <td><%= esod_internal_letter.numer_ewidencyjny %></td>
        <td><%= esod_internal_letter.tytul %></td>
        <td><%= esod_internal_letter.uwagi %></td>
        <td><%= esod_internal_letter.identyfikator_rodzaju_dokumentu_wewnetrznego %></td>
        <td><%= esod_internal_letter.identyfikator_typu_dcmd %></td>
        <td><%= esod_internal_letter.identyfikator_dostepnosci_dokumentu %></td>
        <td><%= esod_internal_letter.pelna_wersja_cyfrowa %></td>
        <td><%= esod_internal_letter.id_zalozyl %></td>
        <td><%= esod_internal_letter.id_aktualizowal %></td>
        <td><%= esod_internal_letter.data_zalozenia %></td>
        <td><%= esod_internal_letter.data_aktualizacji %></td>
        <td><%= esod_internal_letter.initialized_from_esod %></td>
        <td><%= esod_internal_letter.netpar_user %></td>
        <td><%= link_to 'Show', esod_internal_letter %></td>
        <td><%= link_to 'Edit', edit_esod_internal_letter_path(esod_internal_letter) %></td>
        <td><%= link_to 'Destroy', esod_internal_letter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

