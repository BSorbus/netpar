<div class="row">
  <div class="col-sm-12">
    <div class="panel panel-examination">
      <div class="panel-heading">
        <div class="panel-title">
          <span class='<%= t("pages.examination.model_icon") %>'></span>
          <span class='<%= t("pages.examination_#{params[:category_service]}.model_icon") %>'>&nbsp;</span>
          <span><%=t "pages.examination.edit.title"%>:&nbsp;<%= @examination.fullname %></span>
        </div>
      </div>
        <div class="panel-body">
          <div class="col-sm-1">
	        	<h1 class="center"><span class="fa fa-edit"></span></h1> 
          </div> <!--/col-sm-1-->

          <div class="col-sm-11">
						<%= form_for @examination, url: examination_path(@examination, category_service: params[:category_service]) do |f| %>
              <%= render 'form', f: f, back_url: back_url %>
						<% end %>
          </div> <!--/col-sm-11 col-sm-offset-1-->
    
        </div>
    </div> <!-- /panel -->
  </div> <!-- /col-sm-12--> 
</div> <!-- /row -->

