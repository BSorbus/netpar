  <!-- Historia działań -->
  <div class="row">
    <div class="col-sm-10 col-sm-offset-2">

      <div class="panel panel-work">
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_works_inspect">
          <div class="panel-heading">
            <div class="panel-title">
              <span class='<%= t("pages.work.model_icon") %>'>&nbsp;</span>
              <span><%=t "pages.work.index_user_works.title"%>&nbsp;</span>
              <span><i class="indicator glyphicon glyphicon-chevron-right  pull-right"></i></span>
            </div> 
          </div>
        </a>
        <div id="collapse_works_inspect" class="panel-collapse collapse"> <!-- id="collapse_scan_and_print" -->
          <div class="panel-body">
            <div class="col-sm-12">

              <div class="col-sm-12">
                <table id="user-works" class="hover cell-border compact" width="100%" data-source="<%= datatables_index_user_works_path(user_id: @user.id, format: :json) %>">
                  <thead>
                    <tr>
                      <th>0 ID </th>
                      <th class="col-sm-1">1 <%= Work.human_attribute_name(:created_at) %></th>
                      <th class="col-sm-1">2 <%= Work.human_attribute_name(:owner_type) %></th>
                      <th class="col-sm-1">3 <%= Work.human_attribute_name(:action) %></th>
                      <th class="col-sm-1">4 <%= Work.human_attribute_name(:trackable_type) %></th>
                      <th class="col-sm-1">5 <%= Work.human_attribute_name(:trackable_id) %></th>
                      <th>6 <%= Work.human_attribute_name(:parameters) %></th>
                    </tr>
                  </thead>

                  <tbody>                    
                    <!--data from JSON -->
                  </tbody>

                </table>
              </div> <!--/col-sm-12 -->

            </div> <!--/class="col-sm-12" -->
          </div> <!-- /panel-body -->
        </div> <!-- /id="collapse_scan_and_print" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-11 col-sm-offset-1 -->
  </div> <!-- /row -->
