  <!-- Historia działań -->
  <div class="row">
    <div class="col-sm-10 col-sm-offset-2">
      <div class="panel panel-work">
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_works">
          <div class="panel-heading">
            <div class="panel-title">
              <span class='<%= t("pages.work.model_icon") %>'>&nbsp;</span>
              <span><%=t "pages.work.show.title"%></span>
              <i class="indicator glyphicon glyphicon-chevron-right  pull-right"></i>
            </div> 
          </div>
        </a>
        <div id="collapse_works" class="panel-collapse collapse"> <!-- id="collapse_scan_and_print" -->
          <div class="panel-body">
            <div class="col-sm-12">

              <div class="col-sm-12">
                <table id="trackable-works" width="100%" class="hover cell-border compact" width="100%" data-source="<%= datatables_index_trackable_works_path(trackable_id: trackable_id, trackable_type: trackable_type, format: :json) %>">
                  <thead>
                    <tr>
                      <th>0 ID </th>
                      <th>1 <%= Work.human_attribute_name(:created_at) %></th>
                      <th>2 <%= Work.human_attribute_name(:owner_type) %></th>
                      <th>3 <%= Work.human_attribute_name(:action) %></th>
                      <th>4 <%= Work.human_attribute_name(:trackable_type) %></th>
                      <th>5 <%= Work.human_attribute_name(:trackable_id) %></th>
                      <th>6 <%= Work.human_attribute_name(:parameters) %></th>
                    </tr>
                  </thead>

                  <tbody>                    
                    <!--data from JSON -->
                  </tbody>

                </table>
              </div> <!--/col-sm-12 -->

            </div> <!--/class="col-sm-12" -->
          </div> <!-- /panel-body -->
        </div> <!-- /id="collapse_scan_and_print" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-11 col-sm-offset-1 -->
  </div> <!-- /row -->
