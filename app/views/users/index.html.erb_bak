<div class="container">
  <div class="row">
    <h3><span class="glyphicon glyphicon-user" aria-hidden="true"> 
      <%= t "pages.user.index.title" %>  
      </span>
    </h3>
    <div class="column">
      <table class="table table-hover">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
